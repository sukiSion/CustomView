<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MyTextView">
        <attr name="header" format="reference"/>
        <attr name="headerHeight" format="dimension"/>
        <attr name="headerVisableHeight" format="dimension"/>
        <attr name="age">
            <flag name="child" value="10"/>
            <flag name="young" value="18"/>
            <flag name="old" value="60"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomEditText">
        <attr name="ic_delete" format="reference"/>
    </declare-styleable>

    <declare-styleable name="HuaweiClockView">
        <attr name="timeTextSize" format="dimension"/>
        <attr name="timeTextColor" format="color" />
    </declare-styleable>
</resources>