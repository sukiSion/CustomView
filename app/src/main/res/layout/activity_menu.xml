<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".volumeone.three.MenuActivity">



    <ImageView
        android:id="@+id/item_icon_1"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="@id/menu_icon"
        app:layout_constraintTop_toTopOf="@id/menu_icon"
        app:srcCompat="@drawable/circle1" />

    <ImageView
        android:id="@+id/item_icon_2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="@id/menu_icon"
        app:layout_constraintTop_toTopOf="@id/menu_icon"
        app:srcCompat="@drawable/circle2" />

    <ImageView
        android:id="@+id/item_icon_3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="@id/menu_icon"
        app:layout_constraintTop_toTopOf="@id/menu_icon"
        app:srcCompat="@drawable/circle3" />

    <ImageView
        android:id="@+id/item_icon_4"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="@id/menu_icon"
        app:layout_constraintTop_toTopOf="@id/menu_icon"
        app:srcCompat="@drawable/circle4" />

    <ImageView
        android:id="@+id/item_icon_5"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:visibility="gone"
        app:layout_constraintStart_toStartOf="@id/menu_icon"
        app:layout_constraintTop_toTopOf="@id/menu_icon"
        app:srcCompat="@drawable/circle5" />

    <ImageView
        android:id="@+id/menu_icon"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginEnd="20dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/menu" />

</androidx.constraintlayout.widget.ConstraintLayout>